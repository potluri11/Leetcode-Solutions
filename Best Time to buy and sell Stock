class Solution(object):
    def maxProfit(self, prices):
        
        #Set minimum price and max profit
        min_price = 10000
        max_profit = 0
        #Iterate through prices
        for i in range(len(prices)):
            #If you find a price less than min price, update min price
            if prices[i] <= min_price:
                min_price = prices[i]
            else:
                #Find profit between current and min price, if greater than max profit,update max profit
                if max_profit < prices[i] - min_price:
                    max_profit = prices[i] - min_price
        return max_profit
        
            
                
            
